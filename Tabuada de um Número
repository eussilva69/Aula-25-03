#include <stdio.h>
#include <stdlib.h>

int main() {
    int numero, i;

    printf("Digite um n√∫mero inteiro: ");
    scanf("%d", &numero);

    printf("Tabuada de %d:\n", numero);

    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    system("pause");
    return 0;
}
